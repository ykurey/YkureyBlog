<h1>Signins#new</h1>
<p>Find me in app/views/signins/new.html.erb</p>



<%= form_tag signins_path do %>

  <p>
    <%= label_tag :email %>
    <%= text_field_tag :email %>
  </p>
  <p>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </p>
  <p class="button"><%= submit_tag "Log in" %></p>
<% end %>
